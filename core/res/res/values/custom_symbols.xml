<resources>
    <!-- Whether to cancel fingerprint operation if not idle -->
    <java-symbol type="bool" name="config_fpCancelIfNotIdle" />

    <!-- Whether to cleanup fingerprints upon connection to the daemon and when
         user switches -->
    <java-symbol type="bool" name="config_cleanupUnusedFingerprints" />

    <!-- Package Manager -->
    <java-symbol type="array" name="config_deviceDisabledComponents" />
    <java-symbol type="array" name="config_globallyDisabledComponents" />
    <java-symbol type="array" name="config_forceEnabledComponents" />
    
    <!-- Proximity check on screen on -->
    <java-symbol type="bool" name="config_proximityCheckOnWake" />

    <!-- For dynamic default font styles -->
    <java-symbol type="string" name="config_bodyFontFamily" />

    <!-- Proximity check timeout -->
    <java-symbol type="integer" name="config_proximityCheckTimeout" />

    <!-- Proximity check on screen on default -->
    <java-symbol type="bool" name="config_proximityCheckOnWakeEnabledByDefault" />
	    
    <!-- Light / Regular style fonts -->
    <java-symbol type="string" name="config_lightFontFamily" />
    <java-symbol type="string" name="config_regularFontFamily" />

    <!-- Device keyhandlers -->
    <java-symbol type="array" name="config_deviceKeyHandlerLibs" />
    <java-symbol type="array" name="config_deviceKeyHandlerClasses" />

    <!-- Whether device has physical tri state switch -->
    <java-symbol type="bool" name="config_hasAlertSlider" />

    <!-- The location of the devices physical tri state switch -->
    <java-symbol type="integer" name="config_alertSliderLocation" />

    <!-- Whether key handler sends intent when changing slider position -->
    <java-symbol type="string" name="config_alertSliderIntent" />

    <!-- Whether device supports an alt. ambient display -->
    <java-symbol type="bool" name="config_alt_ambient_display"/>

    <!-- SystemUI Restart -->
    <java-symbol type="string" name="systemui_restart_title" />
    <java-symbol type="string" name="systemui_restart_message" />

   <!-- Settings Restart -->
   <java-symbol type="string" name="settings_restart_title" />
   <java-symbol type="string" name="settings_restart_message" />

    <!-- Fingerprint from stock ROM -->
    <java-symbol type="string" name="config_stockFingerprint" />

    <!-- Whether to spoof device as pixel for certain google apps, to
         unlock pixel-exclusive features -->
    <java-symbol type="bool" name="config_spoofGoogleApps" />

    <!-- Device model (Build.MODEL) to spoof for netflix, if not empty -->
    <java-symbol type="string" name="config_netflixSpoofModel" />

    <!-- Power Menu Reboot -->
    <java-symbol name="reboot_to_bootloader_title" type="string" />
    <java-symbol name="reboot_to_bootloader_message" type="string" />
    <java-symbol name="reboot_to_recovery_title" type="string" />
    <java-symbol name="reboot_to_recovery_message" type="string" />
    <java-symbol name="reboot_system_title" type="string" />
    <java-symbol name="reboot_system_message" type="string" />
    <java-symbol type="array" name="config_rebootActionsList" />
	    
    <!-- Whether to force enable multi resolution configuration for camera -->
    <java-symbol type="bool" name="config_forceMultiResolution" />

    <!-- Camera -->
    <java-symbol type="array" name="config_cameraAuxPackageAllowList" />
    <java-symbol type="array" name="config_cameraAuxPackageExcludeList" />
    <java-symbol type="array" name="config_cameraHFRPrivAppList" />

    <!-- Whether device has dash charging support -->
    <java-symbol type="bool" name="config_hasDashCharger" />
    <!-- Path to dash charging status file -->
    <java-symbol type="string" name="config_DashChargerStatusPath" />

    <!-- Whether device has warp charging support -->
    <java-symbol type="bool" name="config_hasWarpCharger" />
    <!-- Paths to warp charging status file -->
    <java-symbol type="string" name="config_WarpChargerStatusPath" />

    <!-- Whether device has VOOC charging support -->
    <java-symbol type="bool" name="config_hasVoocCharger" />

    <!-- Whether device has turbo power charging support -->
    <java-symbol type="bool" name="config_hasTurboPowerCharger" />

    <!-- Whether device has SuperDart charging support -->
    <java-symbol type="bool" name="config_hasSuperDartCharger" />

    <!-- Whether device supports in-display fingerprint when screen is off -->
    <java-symbol type="bool" name="config_supportsScreenOffUdfps" />

</resources>
